{
    "name": "impressiveweb/yandex-disk",
    "description": "A minimal implementation of Yandex Disk Api",
    "homepage": "https://github.com/pavel-macovschi/yandex-disk-api",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Pavel Macovschi",
            "email": "pavel.macovschi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "graham-campbell/guzzle-factory": "^4.0.2|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "ImpressiveWeb\\YandexDisk\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "symfony/var-dumper": "7.1.x-dev"
    }
}
